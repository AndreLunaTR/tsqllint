{
  "name": "tsqllint",
  "version": "1.2.0",
  "description": "Configurable linting for TSQL.",
  "keywords": [
    "TSQL",
    "lint"
  ],
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "postinstall": "node scripts/install/install.js"
  },
  "repository": "tsqllint/tsqllint",
  "files": [
    "scripts/install/install.js",
    "scripts/install/install-dependencies.cmd",
    "scripts/install/install-dependencies.ps1",
    "scripts/install/tsqllint",
    "TSQLLint.Console/bin/Release/TSQLLint.Console.exe"
  ],
  "bin": {
    "tsqllint": "TSQLLint.Console/bin/Release/TSQLLint.Console.exe"
  },
  "os": [
    "win32"
  ],
  "author": "Nathan Boyd & Doug Wilson",
  "license": "MIT"
}
